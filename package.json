{
  "name": "rsr-calculator-service",
  "description": "A Restful service to calculate risk on a known set of parameters.",
  "version": "1.1.13",
  "private": true,
  "author": "Ministry of Justice",
  "license": "MIT",
  "engines": {
    "npm": "~5.6.0",
    "node": "8.9.4"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js | bunyan -o short",
    "test": "./node_modules/.bin/mocha --exit",
    "test-report": "mocha --exit --reporter mocha-junit-reporter",
    "test-coverage": "nyc --silent=true --reporter=lcovonly --reporter=text-lcov npm test && cat ./coverage/lcov.info | codacy-coverage",
    "verify": "swagger project verify",
    "docs-test": "dredd ./api/swagger/swagger.yaml http://127.0.0.1:3000",
    "docs-render": "spectacle -t ./docs ./api/swagger/swagger.yaml",
    "record-build-info": "node ./bin/record-build-info",
    "wait-for-deploy": "node ./bin/wait-for-deploy",
    "snyk": "snyk test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "applicationinsights": "^1.0.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.5",
    "marked": "^0.3.17",
    "moment": "^2.19.4",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "require-all": "^2.2.0",
    "rsr-calculator": "github:noms-digital-studio/rsr-calculator#v1.0.14",
    "sleep-promise": "^2.0.0",
    "statuses": "^1.4.0",
    "swagger-ui-express": "^2.0.13",
    "tough-cookie": "^2.3.3",
    "x-frame-options": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "codacy-coverage": "^2.0.3",
    "date-fns": "^1.29.0",
    "dredd": "^4.1.1",
    "mocha": "^4.0.0",
    "mocha-junit-reporter": "^1.15.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.3.0",
    "snyk": "^1.69.7",
    "spectacle-docs": "^0.9.13",
    "supertest": "^3.0.0",
    "swagger-tools": "^0.10.1"
  },
  "snyk": true
}
